pipeline {
    agent any

    stages {
        stage('Checkout Source') {
            steps {
                git url:'https://github.com/rnrneves/pipeline-gke-gitlab.git', branch 'master'
            }
        }
    
        stage('Build Image') {
            steps {
                script {
                    dockerapp = docker.build("rnrneves/app")

                }
                
            }
        }
    }
    
}